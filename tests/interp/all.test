
init
{
    template InterpretFunction<filename, function-to-run, expected-result>
    {
        run
        {
            return = @($wasm-interp $(input-files)/$(filename) --run $(function-to-run));
        }

        init
        {
            return = @($echo $(expected-result));
        }
    }
}

